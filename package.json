{
  "name": "my-serene-start-page",
  "version": "1.0.0",
  "description": "A beautiful and functional start page with weather, bookmarks, and more",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build": "npx tailwindcss -i ./css/tailwind.css -o ./css/tailwind.min.css --minify",
    "build:watch": "npx tailwindcss -i ./css/tailwind.css -o ./css/tailwind.min.css --watch",
    "serve": "serve . -p 3000",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "keywords": [
    "start-page",
    "dashboard",
    "weather",
    "bookmarks"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
